<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>ServletConf</servlet-name>
        <servlet-class>com.yuan.httpservlet.ServletConf</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>HttpServletDemo1</servlet-name>
        <servlet-class>com.yuan.httpservlet.HttpServletDemo1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InputCodeCheck</servlet-name>
        <servlet-class>com.yuan.httpservlet.InputCodeCheck</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CodeCheck</servlet-name>
        <servlet-class>com.yuan.httpservlet.CodeCheck</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>HttpServletDemo1</servlet-name>
        <url-pattern>/HttpServletDemo1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InputCodeCheck</servlet-name>
        <url-pattern>/InputCodeCheck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CodeCheck</servlet-name>
        <url-pattern>/CodeCheck</url-pattern>
    </servlet-mapping>
</web-app>